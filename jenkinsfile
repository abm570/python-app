pipeline {
    agent any

    

    stages {
        stage('Checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/abm570/python-app.git'
            }
        }

        stage('Compile') {
            steps {
                echo 'Compiling....'
                sh 'python3 app.py'
            }
        }
      }
    }
